
			<!-- begin page-header -->
			<h1 class="page-header">{{body}} <small>Members</small></h1>
			<!-- end page-header -->
    
<!-- begin row -->
			<div class="row">
			    <!-- begin col-12 -->
			    <div class="col-md-12">
			        <!-- begin panel -->
                    <div class="panel panel-inverse">
                        <div class="panel-heading">
                            
                            <h4 class="panel-title">List of members</h4>
                        </div>
                        <div class="panel-body">
                            <table id="data-table" class="table table-striped table-bordered nowrap" width="100%">
                                <thead>
                                    <tr>
                                        <th>Name</th>
																				<th>uid</th>
																				th>Member since</th>
																		    <th>Member until</th>
																		    <!--<th>Gender</th>-->
																		    <th>Mail</th>
																		    
                                    </tr>
                                </thead>
                                <tbody>
                                	{{#each applicant}}
                                    <tr class="odd gradeX">
                                        <td>{{cn}}</td>
																        <td> <a href="/user/{{uid}}">{{uid}}</a> </td> <!--or maybe using <a href> directly on the name?-->
																        <td>{{memberSinceDate}}</td>
																        <td>{{memberUntilDate}}</td>
																        <td>{{mail}}</td>
                                    </tr>
                                    {{/each}} 
                                    
                                </tbody>
                            </table>
                        </div>
                    </div>
                    <!-- end panel -->
                </div>
                <!-- end col-12 -->
            </div>
            <!-- end row -->	
 

<div class="container"> 
  <form id="register" action="/useradd" method="post" class="form-group">
  
  <fieldset> 
    <h2>Personal details</h2> 
     <div class="form-group"> 
        <label>First Name</label>
        <input id="given-name" name="givenName" type="text" placeholder="First name only" class="form-control" required autofocus> 
    
    </div>
   <div class="form-group"> 
        <label>Last Name</label>
        <input id="family-name" name="sn" type="text" placeholder="Last name only" class="form-control" required autofocus> 
    
    </div>
   <div class="form-group"> 
        <label>Contact/Local </label> 
        <input id="antenna" name="antenna" type="text" placeholder="Leuven" class="form-control" required>
        
    </div> 
   <div class="form-group"> 
      <label>Date of Birth</label>
        <input id="dob" name="birthDate" type="date" class="form-control" required>
    
    </div> 
   <div class="form-group"> 
        <label>Email </label> 
        <input id="email" name="mail" type="email" placeholder="example@domain.com" class="form-control"required>
    
    </div> 
   <div class="form-group"> 
        <label>Password </label> 
        <input id="pass" name="userPassword" type="password" placeholder="pippins" class="form-control" required>
    
    </div> 

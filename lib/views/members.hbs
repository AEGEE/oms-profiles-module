

     <div class="bs-docs-header" id="content">
        <div class="container" >  
    <h1>List of members of the body: {{body}}</h1>
        <p> </p>
    <hr>
        
</div>
      </div>
    
<div class="container"> 
  
 
<table class="table table-hover">
  <tr>
    <th>Name</th>
    <th>uid</th>
    <th>Member since</th>
    <th>Member until</th>
    <!--<th>Gender</th>-->
    <th>Mail</th>
    <th>Actions</th>
  </tr>

{{#each member}}
      <tr>
        <td>{{cn}}</td>
        <td> <a href="/user/{{uid}}">{{uid}}</a> </td> <!--or maybe using <a href> directly on the name?-->
        <td>{{memberSinceDate}}</td>
        <td>{{memberUntilDate}}</td>
        <td>{{mail}}</td>

        <td>
                <form action="/members/modifymembership"  method="post" class="form-inline">
          <div class="form-group">
            <input type="hidden" name="uid" value={{uid}}  class="form-control"></input>
            <input type="hidden" name="bodyCode" value={{bodyCode}}  class="form-control"></input>
            <input type="hidden" name="memberType-{{uid}}-{{bodyCode}}" value="Suspended"  class="form-control"></input>
            <div class="radio-inline has-success">
            <label>
              
                <button type="submit" class="btn btn-danger">Suspend</button>
              
            </label>
            </div>
            
          </div>
                </form>
        </td>


      </tr> 
 {{/each}}   
      


      </tr> 
    </br>
</table>   

    </div> <!-- /container -->
  
